{
  "version": 2,
  "name": "mdxcms",
  "alias": "mdxcms.com",
  "builds": [
    {
      "src": "www/next.config.js",
      "use": "@now/next"
    },
    {
      "src": "api/graphql/index.js",
      "use": "@now/node-server"
    },
    {
      "src": "api/rest/**/*.js",
      "use": "@now/node"
    },
    {
      "src": "assets/**/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/api/graphql",
      "dest": "/api/graphql/index.js"
    },
    {
      "src": "/api/users",
      "dest": "/api/rest/users/index.js"
    },
    {
      "src": "/api/user/new",
      "dest": "/api/rest/user/new.js"
    },
    {
      "src": "/api/document",
      "dest": "/api/rest/document.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/www/$1"
    }
  ],
  "env": {
    "JWT_SECRET": "@jwt-token"
  }
}
