{
  "version": 2,
  "name": "mdxcms",
  "alias": "mdxcms.com",
  "builds": [
    {
      "src": "www/next.config.js",
      "use": "@now/next"
    },
    {
      "src": "api/graphql/index.js",
      "use": "@now/node-server"
    },
    {
      "src": "api/rest/**/*.js",
      "use": "@now/node"
    },
    {
      "src": "assets/**/*",
      "use": "@now/static"
    },
    {
      "src": "integration-static/**/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/api/graphql",
      "dest": "/api/graphql/index.js"
    },
    {
      "src": "/api/users",
      "dest": "/api/rest/users/index.js"
    },
    {
      "src": "/api/user/new-from-zeit",
      "dest": "/api/rest/user/new-from-zeit.js"
    },
    {
      "src": "/api/oauth",
      "dest": "/api/rest/oauth/index.js"
    },
    {
      "src": "/api/file/(.*)",
      "dest": "/api/rest/file/index.js?id=$1"
    },
    {
      "src": "/api/document",
      "dest": "/api/rest/document.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/integration-static/(.*)",
      "dest": "/integration-static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/www/$1"
    }
  ],
  "env": {
    "JWT_SECRET": "@jwt-token",
    "NOW_INTEGRATION_SECRET": "@now-integration-secret",
    "NOW_INTEGRATION_ID": "@now-integration-id"
  }
}
